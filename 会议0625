审批模版






SET NAMES utf8mb4;
SET FOREIGN_KEY_CHECKS = 0;
DROP TABLE IF EXISTS `finace_approval_instance_content`;
CREATE TABLE `finace_approval_instance_content`(
  `instance_id`  bigint(19) unsigned NOT NULL COMMENT '审批实例编号',
  `staff_id` bigint(19) unsigned NOT NULL COMMENT '所属用户编号',
  `staff_name` varchar(64) DEFAULT '' COMMENT '所属用户名称',
  `tenant_id` bigint(19) unsigned NOT NULL COMMENT '所属于企业编号',
  `tenant_name` varchar(64) NOT NULL COMMENT '租户名称',
  `finace_amount`  bigint(19) unsigned NOT NULL COMMENT '审批金额，单位 分',
  `create_by` varchar(64) DEFAULT NULL COMMENT '创建者',
  `create_at` datetime DEFAULT NULL COMMENT '创建时间',
  `update_by` varchar(64) DEFAULT NULL COMMENT '更新者',
  `update_at` datetime DEFAULT NULL COMMENT '更新时间',
  `delete_flag` tinyint(1) unsigned DEFAULT '0' COMMENT '删除标记（0:未删除，1:已删除）',
  PRIMARY KEY (`instance_id`)
)ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COMMENT='财务审批内容表';
SET FOREIGN_KEY_CHECKS = 1;




